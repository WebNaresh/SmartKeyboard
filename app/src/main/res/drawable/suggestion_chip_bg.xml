<?xml version="1.0" encoding="utf-8"?>
<ripple xmlns:android="http://schemas.android.com/apk/res/android"
    android:color="#33FFFFFF">
    <item>
        <selector>
            <item android:state_pressed="true">
                <shape android:shape="rectangle">
                    <solid android:color="#FF2A2A3E" />
                    <corners android:radius="16dp" />
                    <stroke android:width="1dp" android:color="#FF3A3A4E" />
                </shape>
            </item>
            <item>
                <shape android:shape="rectangle">
                    <solid android:color="#FF1E1E2E" />
                    <corners android:radius="16dp" />
                    <stroke android:width="1dp" android:color="#FF2E2E3E" />
                </shape>
            </item>
        </selector>
    </item>
</ripple>
